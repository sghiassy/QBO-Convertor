{
  "name": "qbo-converter",
  "version": "0.0.1",
  "description": "Simple node script to convert qfx files to to QBO files for Quickbooks Import",
  "main": "index.js",
  "engines": {
    "node": "20.18.0"
  },
  "scripts": {
    "heroku:deploy": "git push heroku",
    "start": "grunt; node web.js",
    "test": "grunt",
    "docker:context:default": "docker context use default",
    "docker:context:hostinger": "docker context use hostinger",
    "docker:build:image": "npm run docker:context:default && docker build --platform linux/amd64 -t sghiassy/qbo-converter .",
    "docker:push": "docker push sghiassy/qbo-converter:latest",
    "docker:deploy": "npm run docker:context:hostinger && docker stack deploy -c docker-compose.yml qbo-app"
  },
  "repository": "https://github.com/sghiassy/QBO-Convertor",
  "author": "Shaheen Ghiassy",
  "license": "BSD",
  "dependencies": {
    "express": "^4.17.1",
    "jshint": "^2.9.5",
    "morgan": "^1.9.1",
    "optimist": "~0.6.0",
    "path": "^0.12.7",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "grunt": "^1.5.3",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-nodeunit": "^4.0.0",
    "grunt-contrib-uglify": "^5.2.1",
    "grunt-contrib-watch": "^1.1.0"
  }
}
